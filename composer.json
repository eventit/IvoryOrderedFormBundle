{
    "name": "egeloen/ordered-form-bundle",
    "type": "symfony-bundle",
    "description": "Provides a form ordering support for your Symfony2 project.",
    "keywords": [ "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eventit/ivory-ordered-form.git"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/framework-bundle": "^4.4|^5.4",
        "egeloen/ordered-form": "dev-pelikan"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^4.4"
    },
    "autoload": {
        "psr-4": { "Ivory\\OrderedFormBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-php8": "php8-dev"
        }
    }
}
